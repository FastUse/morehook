{
  "editor.tabSize": 2,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "html.validate.styles": false,
  "stylelint.validate": [
    "css",
    "less",
    "scss",
    "vue"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.stylelint": true,
    "source.fixAll.eslint": true
  },
}